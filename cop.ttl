#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/cop/applicableFor
cop:applicableFor rdf:type owl:ObjectProperty ;
                  rdfs:domain cop:PhenotypeAlgorithm ;
                  rdfs:range cop:PhenotypeQuery ;
                  rdfs:label "applicable for"@en .


###  https://w3id.org/cop/basedOn
cop:basedOn rdf:type owl:ObjectProperty ;
            rdfs:domain cop:PhenotypeQuery ;
            rdfs:range cop:PhenotypeModel ;
            rdfs:label "based on"@en .


###  https://w3id.org/cop/basicRestrictionOf
cop:basicRestrictionOf rdf:type owl:ObjectProperty ;
                       rdfs:domain cop:BasicRestrictedPhenotypeClassView ;
                       rdfs:range cop:BasicUnrestrictedPhenotypeClassView ;
                       rdfs:label "basic restriction of"@en .


###  https://w3id.org/cop/derivedFrom
cop:derivedFrom rdf:type owl:ObjectProperty ;
                rdfs:domain cop:DerivedUnrestrictedPhenotypeClassView ;
                rdfs:range cop:PhenotypeClassView ;
                rdfs:label "derived from"@en .


###  https://w3id.org/cop/derivedRestrictionOf
cop:derivedRestrictionOf rdf:type owl:ObjectProperty ;
                         rdfs:domain cop:DerivedRestrictedPhenotypeClassView ;
                         rdfs:range cop:DerivedUnrestrictedPhenotypeClassView ;
                         rdfs:label "derived restriction of"@en .


###  https://w3id.org/cop/hasExclusion
cop:hasExclusion rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cop:hasQueryParameter ;
                 rdfs:label "has exclusion"@en .


###  https://w3id.org/cop/hasInclusion
cop:hasInclusion rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cop:hasQueryParameter ;
                 rdfs:label "has inclusion"@en .


###  https://w3id.org/cop/hasProjection
cop:hasProjection rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf cop:hasQueryParameter ;
                  rdfs:label "has projection"@en .


###  https://w3id.org/cop/hasQueryParameter
cop:hasQueryParameter rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf gfo-light:hasPart ;
                      rdfs:domain cop:PhenotypeQuery ;
                      rdfs:range cop:PhenotypeClassView ;
                      rdfs:label "has query parameter"@en .


###  https://w3id.org/cop/hasVariable
cop:hasVariable rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf gfo-light:hasPart ;
                rdfs:domain cop:DerivationExpression ;
                rdfs:range cop:PhenotypeClassView ;
                rdfs:label "has variable"@en .


###  https://w3id.org/cop/identifies
cop:identifies rdf:type owl:ObjectProperty ;
               rdfs:domain cop:Phenotyping ;
               rdfs:range cop:Organism ;
               rdfs:label "identifies"@en .


###  https://w3id.org/cop/implementedBy
cop:implementedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf cop:implements ;
                  rdfs:domain cop:Algorithm ;
                  rdfs:range cop:Software ;
                  rdfs:label "implemented by"@en .


###  https://w3id.org/cop/implements
cop:implements rdf:type owl:ObjectProperty ;
               rdfs:domain cop:Software ;
               rdfs:range cop:Algorithm ;
               rdfs:label "implements"@en .


###  https://w3id.org/cop/realises
cop:realises rdf:type owl:ObjectProperty ;
             rdfs:domain cop:Software ;
             rdfs:range gfo-light:Process ;
             rdfs:label "realises"@en .


###  https://w3id.org/cop/restrictionOf
cop:restrictionOf rdf:type owl:ObjectProperty ;
                  rdfs:domain cop:RestrictedPhenotypeClass ;
                  rdfs:range cop:UnrestrictedPhenotypeClass ;
                  rdfs:label "restriction of"@en .


###  https://w3id.org/gfo-light/attributiveOf
gfo-light:attributiveOf rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/hasAttributive
gfo-light:hasAttributive rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/hasPart
gfo-light:hasPart rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/instanceOf
gfo-light:instanceOf rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/instantiatedBy
gfo-light:instantiatedBy rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/partOf
gfo-light:partOf rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/represents
gfo-light:represents rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/cop/Algorithm
cop:Algorithm rdf:type owl:Class ;
              rdfs:subClassOf gfo-light:InformationObject ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty cop:implementedBy ;
                                owl:allValuesFrom cop:Software
                              ] ;
              rdfs:label "Algorithm"@en ,
                         "Algorithmus"@de ;
              skos:definition "An algorithm is a specification of instructions to solve a problem or a class of problems."@en ,
                              "Ein Algorithmus ist eine Spezifikation von Anweisungen zur Lösung eines Problems oder einer Klasse von Problemen."@de .


###  https://w3id.org/cop/AttributeSpec
cop:AttributeSpec rdf:type owl:Class ;
                  rdfs:subClassOf gfo-light:InformationObject ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty gfo-light:partOf ;
                                    owl:someValuesFrom cop:PhenotypeClassView
                                  ] ;
                  rdfs:label "Attribute specification"@en ,
                             "Attributspezifikation"@de ;
                  skos:definition "An attribute specification is a specification of an attribute."@en ,
                                  "Eine Attributspezifikation ist eine Spezifikation eines Attributs."@de .


###  https://w3id.org/cop/BasicRestrictedPhenotypeClassView
cop:BasicRestrictedPhenotypeClassView rdf:type owl:Class ;
                                      rdfs:subClassOf cop:PhenotypeClassView ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty cop:basicRestrictionOf ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass cop:BasicUnrestrictedPhenotypeClassView
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty gfo-light:hasPart ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass cop:ValueRangeSpec
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty gfo-light:represents ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass cop:RestrictedPhenotypeClass
                                                      ] ;
                                      rdfs:label "Basic restricted phenotype class view"@en ,
                                                 "Basissicht einer eingeschränkten Phänotypklasse"@de ;
                                      skos:definition "A basic restricted phenotype class view represents a restricted phenotype class that restricts an unrestricted phenotype class represented by a basic view."@en ,
                                                      "Eine Basissicht einer eingeschränkten Phänotypklasse repräsentiert eine eingeschränkte Phänotypklasse, die eine uneingeschränkte Phänotypklasse einschränkt, die durch eine Basissicht repräsentiert wird."@de ;
                                      skos:example "Specification of the phenotype class 'Weight over 100 kg'"@en .


###  https://w3id.org/cop/BasicUnrestrictedPhenotypeClassView
cop:BasicUnrestrictedPhenotypeClassView rdf:type owl:Class ;
                                        rdfs:subClassOf cop:PhenotypeClassView ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:hasPart ;
                                                          owl:someValuesFrom cop:TerminologyCode
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:hasPart ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:DataTypeSpec
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:represents ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:UnrestrictedPhenotypeClass
                                                        ] ;
                                        rdfs:label "Basic unrestricted phenotype class view"@en ,
                                                   "Basissicht einer uneingeschränkten Phänotypklasse"@de ;
                                        skos:definition "A basic unrestricted phenotype class view represents an unrestricted phenotype class whose instances (phenotypes) have already been determined and can be queried from the data source."@en ,
                                                        "Eine Basissicht einer uneingeschränkten Phänotypklasse repräsentiert eine uneingeschränkte Phänotypklasse, deren Instanzen (Phänotypen) bereits bestimmt wurden und von der Datenquelle abgefragt werden können."@de ;
                                        skos:example "Specification of the phenotype class 'Weight'"@en .


###  https://w3id.org/cop/BooleanSpec
cop:BooleanSpec rdf:type owl:Class ;
                rdfs:subClassOf cop:DataTypeSpec ;
                rdfs:label "Boolean data type specification"@en .


###  https://w3id.org/cop/DataTypeSpec
cop:DataTypeSpec rdf:type owl:Class ;
                 rdfs:subClassOf cop:AttributeSpec ;
                 rdfs:label "Data type specification"@en ,
                            "Datentypspezifikation"@de ;
                 skos:definition "A data type specification is a specification of a data type (e.g., string, decimal, boolean or date-time)."@en ,
                                 "Eine Datentypspezifikation ist eine Spezifikation eines Datentyps (z.B. Text, Zahl, Boolescher Wert oder Datum-Uhrzeit)."@de .


###  https://w3id.org/cop/DateTimeSpec
cop:DateTimeSpec rdf:type owl:Class ;
                 rdfs:subClassOf cop:DataTypeSpec ;
                 rdfs:label "Date-time data type specification"@en .


###  https://w3id.org/cop/DecimalSpec
cop:DecimalSpec rdf:type owl:Class ;
                rdfs:subClassOf cop:DataTypeSpec ;
                rdfs:label "Decimal data type specification"@en .


###  https://w3id.org/cop/DerivationExpression
cop:DerivationExpression rdf:type owl:Class ;
                         rdfs:subClassOf cop:AttributeSpec ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty cop:hasVariable ;
                                           owl:someValuesFrom cop:PhenotypeClassView
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty gfo-light:partOf ;
                                           owl:someValuesFrom cop:DerivedUnrestrictedPhenotypeClassView
                                         ] ;
                         rdfs:label "Ableitungsausdruck"@de ,
                                    "Derivation expression"@en ;
                         skos:definition "A derivation expression is an executable formal specification of a combination of different functions/operators and input variables, the execution of which calculates/derives a new value (e.g., of a phenotype) based on the input values."@en ,
                                         "Ein Ableitungsausdruck ist eine ausführbare formale Spezifikation einer Kombination aus verschiedenen Funktionen/Operatoren und Eingabevariablen, deren Ausführung einen neuen Wert (z.B. eines Phänotyps) auf der Grundlage der Eingabewerte berechnet/ableitet."@de .


###  https://w3id.org/cop/DerivedRestrictedPhenotypeClassView
cop:DerivedRestrictedPhenotypeClassView rdf:type owl:Class ;
                                        rdfs:subClassOf cop:PhenotypeClassView ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cop:derivedRestrictionOf ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:DerivedUnrestrictedPhenotypeClassView
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:hasPart ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:ValueRangeSpec
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:represents ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:RestrictedPhenotypeClass
                                                        ] ;
                                        rdfs:label "Ableitungssicht einer eingeschränkten Phänotypklasse"@de ,
                                                   "Derived restricted phenotype class view"@en ;
                                        skos:definition "A derived restricted phenotype class view represents a restricted phenotype class that restricts an unrestricted phenotype class represented by a derived view."@en ,
                                                        "Eine Ableitungssicht einer eingeschränkten Phänotypklasse repräsentiert eine eingeschränkte Phänotypklasse, die eine uneingeschränkte Phänotypklasse einschränkt, die durch eine Ableitungssicht repräsentiert wird."@de ;
                                        skos:example "Specification of the phenotype class 'Overweight (e.g., BMI over 30)'"@en .


###  https://w3id.org/cop/DerivedUnrestrictedPhenotypeClassView
cop:DerivedUnrestrictedPhenotypeClassView rdf:type owl:Class ;
                                          rdfs:subClassOf cop:PhenotypeClassView ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty cop:derivedFrom ;
                                                            owl:someValuesFrom cop:PhenotypeClassView
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gfo-light:hasPart ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass cop:DerivationExpression
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gfo-light:represents ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass cop:UnrestrictedPhenotypeClass
                                                          ] ;
                                          rdfs:label "Ableitungssicht einer uneingeschränkten Phänotypklasse"@de ,
                                                     "Derived unrestricted phenotype class view"@en ;
                                          skos:definition "A derived unrestricted phenotype class view represents an unrestricted phenotype class whose instances (phenotypes) need to be determined (i.e., derived from other phenotypes)."@en ,
                                                          "Eine Ableitungssicht einer uneingeschränkten Phänotypklasse repräsentiert eine uneingeschränkte Phänotypklasse, deren Instanzen (Phänotypen) bestimmt (d.h. von anderen Phänotypen abgeleitet) werden müssen."@de ;
                                          skos:example "Specification of the phenotype class 'Body Mass Index (BMI)'"@en .


###  https://w3id.org/cop/Description
cop:Description rdf:type owl:Class ;
                rdfs:subClassOf cop:AttributeSpec ;
                rdfs:label "Beschreibung"@de ,
                           "Description"@en ;
                skos:definition "A description is a string describing an entity."@en ,
                                "Eine Beschreibung ist eine Zeichenkette, die eine Entität beschreibt."@de .


###  https://w3id.org/cop/DynamicPhenotypeAlgorithm
cop:DynamicPhenotypeAlgorithm rdf:type owl:Class ;
                              rdfs:subClassOf cop:PhenotypeAlgorithm ;
                              rdfs:label "Dynamic phenotype algorithm"@en ,
                                         "Dynamischer Phänotypalgorithmus"@de ;
                              skos:definition "A dynamic phenotype algorithm is a phenotype algorithm for executing an arbitrary phenotype query."@en ,
                                              "Ein dynamischer Phänotypalgorithmus ist ein Phänotypalgorithmus für die Ausführung einer beliebigen Phänotypabfrage."@de .


###  https://w3id.org/cop/Model
cop:Model rdf:type owl:Class ;
          rdfs:subClassOf gfo-light:InformationObject ;
          rdfs:label "Model"@en ,
                     "Modell"@de ;
          skos:definition "A model is a representation/specification of an abstraction of an entity."@en ,
                          "Ein Modell ist eine Repräsentation/Spezifikation einer Abstraktion einer Entität."@de .


###  https://w3id.org/cop/Organism
cop:Organism rdf:type owl:Class ;
             rdfs:subClassOf gfo-light:Object ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty gfo-light:hasAttributive ;
                               owl:someValuesFrom cop:Phenotype
                             ] ;
             rdfs:label "Organism"@en ,
                        "Organismus"@de ;
             skos:definition "An organism is a living object."@en ,
                             "Ein Organismus ist ein lebendes Objekt."@de .


###  https://w3id.org/cop/Phenotype
cop:Phenotype rdf:type owl:Class ;
              rdfs:subClassOf gfo-light:Attributive ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gfo-light:instanceOf ;
                                owl:someValuesFrom cop:PhenotypeClass
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gfo-light:attributiveOf ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass cop:Organism
                              ] ;
              rdfs:label "Phenotype"@en ,
                         "Phänotyp"@de ;
              skos:definition "A phenotype is a (combination of) bodily feature(s) or observable characteristic(s)/trait(s) of an organism."@en ,
                              "Ein Phänotyp ist eine (Kombination von) körperliche(n) Eigenschaft(en) oder beobachtbare(s) Merkmal(e)/Eigenschaft(en) eines Organismus."@de ;
              skos:example "Body Mass Index (BMI) of a specific person"@en ,
                           "Overweight of a specific person"@en ,
                           "Weight of a specific person"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource cop:Phenotype ;
   owl:annotatedProperty skos:definition ;
   owl:annotatedTarget "A phenotype is a (combination of) bodily feature(s) or observable characteristic(s)/trait(s) of an organism."@en ;
   terms:references "Mahner M, Kary M. What Exactly Are Genomes, Genotypes and Phenotypes? And What About Phenomes? J Theor Biol 1997;186:55–63. https://doi.org/10.1006/jtbi.1996.0335."@en ,
                    "Scheuermann RH, Ceusters W, Smith B. Toward an Ontological Treatment of Disease and Diagnosis. Summit Transl Bioinforma 2009;2009:116–20. https://pmc.ncbi.nlm.nih.gov/articles/PMC3041577/"@en
 ] .


###  https://w3id.org/cop/PhenotypeAlgorithm
cop:PhenotypeAlgorithm rdf:type owl:Class ;
                       rdfs:subClassOf cop:Algorithm ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty cop:applicableFor ;
                                         owl:someValuesFrom cop:PhenotypeQuery
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty cop:implementedBy ;
                                         owl:allValuesFrom cop:PhenotypingSoftware
                                       ] ;
                       rdfs:label "Phenotype algorithm"@en ,
                                  "Phänotypalgorithmus"@de ;
                       skos:definition "A phenotype algorithm is an algorithm for executing a phenotype query, i.e., identifying individuals matching the query criteria (i.e., meeting all the inclusion but none of the exclusion criteria) and deriving/outputting further phenotypes (projection entries)."@en ,
                                       "Ein Phänotypalgorithmus ist ein Algorithmus zur Ausführung einer Phänotypabfrage, d.h. zur Identifizierung von Individuen, die den Abfragekriterien entsprechen (d.h. alle Einschluss-, aber keines der Ausschlusskriterien erfüllen) und zur Ableitung/Ausgabe weiterer Phänotypen (Projektionseinträge)."@de .


###  https://w3id.org/cop/PhenotypeClass
cop:PhenotypeClass rdf:type owl:Class ;
                   rdfs:subClassOf gfo-light:Category ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gfo-light:instantiatedBy ;
                                     owl:allValuesFrom cop:Phenotype
                                   ] ;
                   rdfs:label "Phenotype class"@en ,
                              "Phänotypklasse"@de ;
                   skos:definition "Phenotype classes are categories whose instances are phenotypes."@en ,
                                   "Phänotypklassen sind Kategorien, deren Instanzen Phänotypen sind."@de .


###  https://w3id.org/cop/PhenotypeClassView
cop:PhenotypeClassView rdf:type owl:Class ;
                       rdfs:subClassOf cop:Model ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty gfo-light:hasPart ;
                                         owl:someValuesFrom cop:Title
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty gfo-light:represents ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass cop:PhenotypeClass
                                       ] ;
                       rdfs:label "Phenotype class view"@en ,
                                  "Phänotypklassensicht"@de ;
                       skos:definition "A phenotype class view is a formal representation of a phenotype class (its attributes and relationships)."@en ,
                                       "Eine Phänotypklassensicht ist eine formale Repräsentation einer Phänotypklasse (ihrer Attribute und Relationen)."@de .


###  https://w3id.org/cop/PhenotypeModel
cop:PhenotypeModel rdf:type owl:Class ;
                   rdfs:subClassOf cop:Model ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gfo-light:hasPart ;
                                     owl:someValuesFrom cop:PhenotypeClassView
                                   ] ;
                   rdfs:label "Phenotype model"@en ,
                              "Phänotypmodell"@de ;
                   skos:definition "A phenotype model is a formal representation of delimited phenotypic knowledge (i.e., phenotype classes, their attributes, and relationships)."@en ,
                                   "Ein Phänotypmodell ist eine formale Darstellung von abgegrenztem phänotypischem Wissen (d.h. von Phänotypklassen, ihren Attributen und Beziehungen)."@de .


###  https://w3id.org/cop/PhenotypeQuery
cop:PhenotypeQuery rdf:type owl:Class ;
                   rdfs:subClassOf cop:Model ,
                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty cop:hasExclusion ;
                                                     owl:someValuesFrom cop:PhenotypeClassView
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty cop:hasInclusion ;
                                                     owl:someValuesFrom cop:PhenotypeClassView
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty cop:hasProjection ;
                                                     owl:someValuesFrom cop:PhenotypeClassView
                                                   ]
                                                 )
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty cop:basedOn ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass cop:PhenotypeModel
                                   ] ;
                   rdfs:label "Phenotype query"@en ,
                              "Phänotypabfrage"@de ;
                   skos:definition "A phenotype query is a specification of certain phenotype classes as inclusion or exclusion criteria or as projection (output) entries."@en ,
                                   "Eine Phänotypabfrage ist eine Spezifikation bestimmter Phänotypklassen als Ein- oder Ausschlusskriterien oder als Projektions- (Ausgabe-) Einträge."@de .


###  https://w3id.org/cop/Phenotyping
cop:Phenotyping rdf:type owl:Class ;
                rdfs:subClassOf gfo-light:Process ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty cop:identifies ;
                                  owl:allValuesFrom cop:Organism
                                ] ;
                rdfs:label "Phenotyping"@en ,
                           "Phänotypisierung"@de ;
                skos:definition "Die Phänotypisierung ist ein Prozess der Identifizierung von Personen mit bestimmten Phänotypen."@de ,
                                "Phenotyping is a process of identifying individuals with certain phenotypes."@en .


###  https://w3id.org/cop/PhenotypingSoftware
cop:PhenotypingSoftware rdf:type owl:Class ;
                        rdfs:subClassOf cop:Software ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty cop:implements ;
                                          owl:someValuesFrom cop:PhenotypeAlgorithm
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty cop:realises ;
                                          owl:someValuesFrom cop:Phenotyping
                                        ] ;
                        rdfs:comment "A run of a phenotyping software (implemented phenotype algorithm) realises a phenotyping process."@en ,
                                     "Ein Durchlauf einer Phänotypisierungssoftware (implementierter Phänotypisierungsalgorithmus) realisiert einen Phänotypisierungsprozess."@de ;
                        rdfs:label "Phenotyping software"@en ,
                                   "Phänotypisierungssoftware"@de ;
                        skos:definition "Phenotyping software is a software implementing a phenotype algorithm."@en ,
                                        "Phänotypisierungssoftware ist eine Software, die einen Phänotypisierungsalgorithmus implementiert."@de .


###  https://w3id.org/cop/RestrictedPhenotypeClass
cop:RestrictedPhenotypeClass rdf:type owl:Class ;
                             rdfs:subClassOf cop:PhenotypeClass ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty cop:restrictionOf ;
                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                               owl:onClass cop:UnrestrictedPhenotypeClass
                                             ] ;
                             rdfs:label "Eingeschränkte Phänotypklasse"@de ,
                                        "Restricted phenotype class"@en ;
                             skos:altLabel "Phenotype restriction"@en ,
                                           "Phänotyprestriktion"@de ;
                             skos:definition "A phenotype class whose extension (the set of instances, i.e., phenotypes) is restricted to a certain value range is called restricted phenotype class."@en ,
                                             "Eine Phänotypklasse, deren Erweiterung (die Menge der Instanzen, d.h. der Phänotypen) auf einen bestimmten Wertebereich beschränkt ist, wird als eingeschränkte Phänotypklasse bezeichnet."@de ;
                             skos:example "Overweight (e.g., BMI over 30)"@en ,
                                          "Weight over 100 kg"@en .


