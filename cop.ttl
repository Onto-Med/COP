#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/cop/applicableFor
cop:applicableFor rdf:type owl:ObjectProperty ;
                  rdfs:domain cop:PhenotypeAlgorithm ;
                  rdfs:range cop:PhenotypeQuery .


###  https://w3id.org/cop/basicRestrictionOf
cop:basicRestrictionOf rdf:type owl:ObjectProperty ;
                       rdfs:domain cop:BasicRestrictedPhenotypeClassView ;
                       rdfs:range cop:BasicUnrestrictedPhenotypeClassView .


###  https://w3id.org/cop/derivedFrom
cop:derivedFrom rdf:type owl:ObjectProperty ;
                rdfs:domain cop:DerivedUnrestrictedPhenotypeClassView ;
                rdfs:range cop:PhenotypeClassView .


###  https://w3id.org/cop/derivedRestrictionOf
cop:derivedRestrictionOf rdf:type owl:ObjectProperty ;
                         rdfs:domain cop:DerivedRestrictedPhenotypeClassView ;
                         rdfs:range cop:DerivedUnrestrictedPhenotypeClassView .


###  https://w3id.org/cop/hasExclusion
cop:hasExclusion rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cop:hasQueryParameter .


###  https://w3id.org/cop/hasInclusion
cop:hasInclusion rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf cop:hasQueryParameter .


###  https://w3id.org/cop/hasPhenotype
cop:hasPhenotype rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf gfo-light:hasAttributive ;
                 owl:inverseOf cop:phenotypeOf ;
                 rdfs:domain ncit:C14250 ;
                 rdfs:range cop:Phenotype .


###  https://w3id.org/cop/hasProjection
cop:hasProjection rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf cop:hasQueryParameter .


###  https://w3id.org/cop/hasQueryParameter
cop:hasQueryParameter rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf gfo-light:hasPart ;
                      rdfs:domain cop:PhenotypeQuery ;
                      rdfs:range cop:PhenotypeClassView .


###  https://w3id.org/cop/implementedBy
cop:implementedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf cop:implements ;
                  rdfs:domain ncit:C16275 ;
                  rdfs:range ncit:C17146 .


###  https://w3id.org/cop/implements
cop:implements rdf:type owl:ObjectProperty ;
               rdfs:domain ncit:C17146 ;
               rdfs:range ncit:C16275 .


###  https://w3id.org/cop/phenotypeOf
cop:phenotypeOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf gfo-light:attributiveOf ;
                rdfs:domain cop:Phenotype ;
                rdfs:range ncit:C14250 .


###  https://w3id.org/cop/realises
cop:realises rdf:type owl:ObjectProperty ;
             rdfs:domain ncit:C17146 ;
             rdfs:range gfo-light:Process .


###  https://w3id.org/cop/represents
cop:represents rdf:type owl:ObjectProperty ;
               rdfs:domain cop:PhenotypeClassView ;
               rdfs:range cop:PhenotypeClass .


###  https://w3id.org/cop/restrictionOf
cop:restrictionOf rdf:type owl:ObjectProperty ;
                  rdfs:domain cop:RestrictedPhenotypeClass ;
                  rdfs:range cop:UnrestrictedPhenotypeClass .


###  https://w3id.org/gfo-light/attributiveOf
gfo-light:attributiveOf rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/hasAttributive
gfo-light:hasAttributive rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/instanceOf
gfo-light:instanceOf rdf:type owl:ObjectProperty .


###  https://w3id.org/gfo-light/instantiatedBy
gfo-light:instantiatedBy rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C14250
ncit:C14250 rdf:type owl:Class ;
            rdfs:subClassOf gfo-light:Continuant ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty cop:hasPhenotype ;
                              owl:someValuesFrom cop:Phenotype
                            ] ;
            rdfs:label "Organism"@en ;
            skos:definition "A living entity."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16275
ncit:C16275 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationEntity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty cop:implementedBy ;
                              owl:allValuesFrom ncit:C17146
                            ] ;
            rdfs:label "Algorithm"@en ;
            skos:definition "A defined procedure for solving a problem. Applied to a problem-solving procedure implemented in software to be executed by a computer."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16866
ncit:C16866 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationEntity ;
            rdfs:label "Model"@en ;
            skos:definition "A representation of something, often idealized or modified to make it conceptually easier to understand."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C17146
ncit:C17146 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationEntity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty cop:implements ;
                              owl:someValuesFrom ncit:C16275
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty cop:realises ;
                              owl:someValuesFrom gfo-light:Process
                            ] ;
            rdfs:label "Computer Program"@en ;
            skos:definition "A set of coded instructions, which a computer follows in processing data, performing an operation, or solving a logical problem, upon execution of the program."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C19067
ncit:C19067 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ;
            rdfs:label "Title"@en ;
            skos:definition "An identifying designation."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C25162
ncit:C25162 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ;
            rdfs:label "Code"@en ;
            skos:definition "A symbol or combination of symbols which is assigned to the members of a collection."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C25365
ncit:C25365 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ;
            rdfs:label "Description"@en ;
            skos:definition "A written or verbal account, representation, statement, or explanation of something."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C25709
ncit:C25709 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ;
            rdfs:label "Unit of Measure"@en ;
            skos:definition "A named quantity in terms of which other quantities are measured or specified, used as a standard measurement of like kinds."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C38013
ncit:C38013 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty gfo-light:partOf ;
                              owl:someValuesFrom [ rdf:type owl:Class ;
                                                   owl:unionOf ( cop:BasicRestrictedPhenotypeClassView
                                                                 cop:DerivedRestrictedPhenotypeClassView
                                                               )
                                                 ]
                            ] ;
            rdfs:label "Range"@en ;
            skos:definition "The difference between the lowest and highest numerical values; the limits or scale of variation."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C42645
ncit:C42645 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ;
            rdfs:label "Data Type"@en ;
            skos:definition "An indication of the form that a value will have. Examples include string, integer, and character."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C45253
ncit:C45253 rdf:type owl:Class ;
            rdfs:subClassOf ncit:C42645 ;
            rdfs:label "String"@en ;
            skos:definition "An expression consisting of a linear sequence of symbols (characters or words or phrases)."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C45254
ncit:C45254 rdf:type owl:Class ;
            rdfs:subClassOf ncit:C42645 ;
            rdfs:label "Boolean"@en ;
            skos:definition "The type of an expression with two possible values, \"true\" and \"false\"."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C48150
ncit:C48150 rdf:type owl:Class ;
            rdfs:subClassOf ncit:C42645 ;
            rdfs:label "Float"@en ;
            skos:definition "A number that can have its decimal point in any position."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C52469
ncit:C52469 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ;
            rdfs:label "Synonym"@en ;
            skos:definition "A word having the same or nearly the same meaning as another word or other words in a language; a word or an expression that serves as a figurative or symbolic substitute for another."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C54086
ncit:C54086 rdf:type owl:Class ;
            rdfs:subClassOf ncit:C42645 ;
            rdfs:label "Timestamp Data Type"@en ;
            skos:definition "A variation of the Date datatype that's useful for storing very precise datetime values, down to small fractions of seconds. The Timestamp datatype stores all the information that the Date datatype stores, but also includes fractional seconds."@en .


###  http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C93550
ncit:C93550 rdf:type owl:Class ;
            rdfs:subClassOf cop:RepresentationAttribute ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty gfo-light:partOf ;
                              owl:someValuesFrom cop:DerivedUnrestrictedPhenotypeClassView
                            ] ;
            rdfs:label "Derivation Expression"@en ;
            skos:definition "A character string containing a formal language expression that specifies how attributes are, should be, or have been derived from input parameters associated with activity."@en .


###  https://w3id.org/cop/BasicRestrictedPhenotypeClassView
cop:BasicRestrictedPhenotypeClassView rdf:type owl:Class ;
                                      rdfs:subClassOf cop:PhenotypeClassView ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty cop:basicRestrictionOf ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass cop:BasicUnrestrictedPhenotypeClassView
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty cop:represents ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass cop:RestrictedPhenotypeClass
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty gfo-light:hasPart ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass ncit:C38013
                                                      ] ;
                                      rdfs:label "Basic phenotype restriction"@en ,
                                                 "Basisphänotypeinschränkung"@de ;
                                      skos:definition "A basic phenotype restriction is a restriction of a basic phenotype class."@en ,
                                                      "Eine Basisphänotypeinschränkung ist eine Einschränkung einer Basisphänotypklasse."@de .


###  https://w3id.org/cop/BasicUnrestrictedPhenotypeClassView
cop:BasicUnrestrictedPhenotypeClassView rdf:type owl:Class ;
                                        rdfs:subClassOf cop:PhenotypeClassView ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:hasPart ;
                                                          owl:someValuesFrom ncit:C25162
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cop:represents ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:UnrestrictedPhenotypeClass
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:hasPart ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass ncit:C42645
                                                        ] ;
                                        rdfs:label "Basic phenotype class"@en ,
                                                   "Basisphänotypklasse"@de ;
                                        skos:definition "Basic phenotype classes represent phenotypes that have already been determined and for which the data are available."@en ,
                                                        "Basisphänotypklassen stellen Phänotypen dar, die bereits bestimmt wurden und für die Daten verfügbar sind."@de .


###  https://w3id.org/cop/DerivedRestrictedPhenotypeClassView
cop:DerivedRestrictedPhenotypeClassView rdf:type owl:Class ;
                                        rdfs:subClassOf cop:PhenotypeClassView ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cop:derivedRestrictionOf ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:DerivedUnrestrictedPhenotypeClassView
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty cop:represents ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass cop:RestrictedPhenotypeClass
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gfo-light:hasPart ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass ncit:C38013
                                                        ] ;
                                        rdfs:label "Abgeleitete Phänotypeinschränkung"@de ,
                                                   "Derived phenotype restriction"@en ;
                                        skos:definition "A derived phenotype restriction is a restriction of a derived phenotype class."@en ,
                                                        "Eine abgeleitete Phänotypeinschränkung ist eine Einschränkung einer abgeleiteten Phänotypklasse."@de .


###  https://w3id.org/cop/DerivedUnrestrictedPhenotypeClassView
cop:DerivedUnrestrictedPhenotypeClassView rdf:type owl:Class ;
                                          rdfs:subClassOf cop:PhenotypeClassView ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty cop:derivedFrom ;
                                                            owl:someValuesFrom cop:PhenotypeClassView
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty cop:represents ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass cop:UnrestrictedPhenotypeClass
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gfo-light:hasPart ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass ncit:C93550
                                                          ] ;
                                          rdfs:label "Abgeleitete Phänotypklasse"@de ,
                                                     "Derived phenotype class"@en ;
                                          skos:definition "Abgeleitete Phänotypklassen modellieren Phänotypen, die aus anderen Phänotypen abgeleitet werden sollten und daher die Formulierung eines Ableitungsausdrucks/einer Ableitungsregel/einer Ableitungsformel erfordern."@de ,
                                                          "Derived phenotype classes model phenotypes that are to be deduced from other phenotypes and thus require a derivation expression/rule/formula to be formulated."@en .


###  https://w3id.org/cop/DynamicPhenotypeAlgorithm
cop:DynamicPhenotypeAlgorithm rdf:type owl:Class ;
                              rdfs:subClassOf cop:PhenotypeAlgorithm ;
                              rdfs:label "Dynamic phenotype algorithm"@en ,
                                         "Dynamischer Phänotyp-Algorithmus"@de ;
                              skos:definition "A dynamic phenotype algorithm is a clear specification of instructions for executing an arbitrary phenotype query."@en ,
                                              "Ein dynamischer Phänotyp-Algorithmus ist eine klare Spezifikation von Anweisungen für die Ausführung einer beliebigen Phänotyp-Abfrage."@de .


###  https://w3id.org/cop/Phenotype
cop:Phenotype rdf:type owl:Class ;
              rdfs:subClassOf gfo-light:Attributive ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gfo-light:instanceOf ;
                                owl:someValuesFrom cop:PhenotypeClass
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty cop:phenotypeOf ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ncit:C14250
                              ] ;
              rdfs:label "Phenotype"@en ,
                         "Phänotyp"@de ;
              skos:definition "A phenotype is a (combination of) bodily feature(s) or observable characteristic(s)/trait(s) of an organism."@en ,
                              "Ein Phänotyp ist eine (Kombination von) körperliche(n) Eigenschaft(en) oder beobachtbare(s) Merkmal(e)/Eigenschaft(en) eines Organismus."@de .

[ rdf:type owl:Axiom ;
   owl:annotatedSource cop:Phenotype ;
   owl:annotatedProperty skos:definition ;
   owl:annotatedTarget "A phenotype is a (combination of) bodily feature(s) or observable characteristic(s)/trait(s) of an organism."@en ;
   cop:ref "Mahner M, Kary M. What Exactly Are Genomes, Genotypes and Phenotypes? And What About Phenomes? J Theor Biol 1997;186:55–63. https://doi.org/10.1006/jtbi.1996.0335." ,
           "Scheuermann RH, Ceusters W, Smith B. Toward an Ontological Treatment of Disease and Diagnosis. Summit Transl Bioinforma 2009;2009:116–20."
 ] .


###  https://w3id.org/cop/PhenotypeAlgorithm
cop:PhenotypeAlgorithm rdf:type owl:Class ;
                       rdfs:subClassOf ncit:C16275 ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty cop:applicableFor ;
                                         owl:someValuesFrom cop:PhenotypeQuery
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty cop:implementedBy ;
                                         owl:allValuesFrom cop:PhenotypingSoftware
                                       ] ;
                       rdfs:label "Phenotype algorithm"@en ,
                                  "Phänotyp-Algorithmus"@de ;
                       skos:definition "A phenotype algorithm is a clear specification of instructions for executing a phenotype query, i.e., identifying individuals matching the query criteria (i.e., meeting all the inclusion but none of the exclusion criteria) and deriving/outputting further phenotypes (projection entries)."@en ,
                                       "Ein Phänotyp-Algorithmus ist eine klare Spezifikation von Anweisungen zur Ausführung einer Phänotyp-Abfrage, d.h. zur Identifizierung von Individuen, die den Abfragekriterien entsprechen (d.h. alle Einschluss-, aber keines der Ausschlusskriterien erfüllen) und zur Ableitung/Ausgabe weiterer Phänotypen (Projektionseinträge)."@de .


###  https://w3id.org/cop/PhenotypeClass
cop:PhenotypeClass rdf:type owl:Class ;
                   rdfs:subClassOf gfo-light:Category ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gfo-light:instantiatedBy ;
                                     owl:allValuesFrom cop:Phenotype
                                   ] ;
                   rdfs:label "Phenotype class"@en ,
                              "Phänotypklasse"@de ;
                   skos:definition "Phenotype classes are categories whose instances are phenotypes."@en ,
                                   "Phänotypklassen sind Kategorien, deren Instanzen Phänotypen sind."@de .


###  https://w3id.org/cop/PhenotypeClassView
cop:PhenotypeClassView rdf:type owl:Class ;
                       rdfs:subClassOf ncit:C16866 ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty gfo-light:hasPart ;
                                         owl:someValuesFrom ncit:C19067
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty cop:represents ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass cop:PhenotypeClass
                                       ] ;
                       rdfs:label "Phenotype class view"@en ,
                                  "Sicht der Phänotypklasse"@de .


###  https://w3id.org/cop/PhenotypeModel
cop:PhenotypeModel rdf:type owl:Class ;
                   rdfs:subClassOf ncit:C16866 ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gfo-light:hasPart ;
                                     owl:someValuesFrom cop:PhenotypeClassView
                                   ] ;
                   rdfs:label "Phenotype model"@en ,
